@model IEnumerable<UserIdentity.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
}

<div class="panel-primary">
    <div class="panel-heading">
        Users
    </div>
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
        </tr>



        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.UserName</td>
                <td>@item.Email</td>
            </tr>
        }


    </table>
</div>

